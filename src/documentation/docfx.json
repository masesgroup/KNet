{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../net/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "xref": [
      "https://cliparser.masesgroup.com/xrefmap.yml",
      "https://jnet.masesgroup.com/xrefmap.yml"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
	  "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_appTitle": "MASES Group - KNet",
      "_appFooter": "<div style=\"text-align: center;\">Copyright Â© 2024 MASES s.r.l.. | Via San Cesidio 16 - San Valentino in A.C. - PE | P.I. 01932310681</div>",
      "_description": "This is a descriptive page of KNet, the .NET suite for Apache Kafka",
	  "_lang": "en",
      "_gitContribute": {

      }
    },
    "dest": "../../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}