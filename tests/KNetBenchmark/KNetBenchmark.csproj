<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>KNetBenchmark</AssemblyName>
    <OutputType>Exe</OutputType>
    <RootNamespace>MASES.KNet.Benchmark</RootNamespace>
    <Title>KNetTest - a test tool for KNet</Title>
    <Description>KNetTest - a test tool for KNet</Description>
    <Copyright>Copyright Â©  MASES s.r.l. 2022</Copyright>
    <Authors>MASES s.r.l.</Authors>
    <Company>MASES s.r.l.</Company>
    <Version>1.2.2.0</Version>
    <TargetFrameworks>net461;netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
    <OutputPath>..\..\bin\</OutputPath>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net461'">
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Confluent.Kafka" Version="1.8.2" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\src\net\KNet\KNet.csproj" />
  </ItemGroup>
</Project>
